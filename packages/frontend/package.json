{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "npx rimraf node_modules package-lock.json .next out .open-next",
    "setup": "npm run clean && npm install",
    "dev:local": "cross-env PROVIDER_MODE=local env-cmd -f .env next dev",
    "dev:goerli": "cross-env PROVIDER_MODE=goerli env-cmd -f .env next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "npx serve out",
    "tsc": "bash -c tsc --project ./tsconfig.json",
    "prettier": "npx prettier --write .",
    "lint": "npx eslint ."
  },
  "dependencies": {
    "@types/node": "20.6.4",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@wagmi/core": "^1.4.2",
    "autoprefixer": "10.4.16",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.2",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.10",
    "next": "13.5.2",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "viem": "^1.12.2",
    "wagmi": "^1.4.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "serve": "^14.2.1"
  }
